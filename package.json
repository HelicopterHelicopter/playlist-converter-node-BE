{
  "name": "playlist-converter-node-backend",
  "version": "1.0.0",
  "description": "Backend API for YouTube to Spotify Playlist Converter using Node.js/Express",
  "main": "server.js",
  "scripts": {
    "start:node": "node server.js",
    "dev": "nodemon server.js",
    "start": "pm2 start ecosystem.config.js --no-daemon",
    "start:prod": "pm2 start ecosystem.config.js --env production",
    "stop": "pm2 stop ecosystem.config.js",
    "restart": "pm2 restart ecosystem.config.js",
    "delete": "pm2 delete ecosystem.config.js",
    "list": "pm2 list",
    "logs": "pm2 logs playlist-converter-backend"
  },
  "keywords": [
    "spotify",
    "youtube",
    "playlist",
    "converter",
    "api",
    "express",
    "node"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "googleapis": "^140.0.1",
    "spotify-web-api-node": "^5.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  }
} 